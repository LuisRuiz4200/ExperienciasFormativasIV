@model Dominio.Entidad.MantenimientoModel

@{
    ViewBag.Title = "detalleMantenimiento";
}

<h2>DETALLE DE MANTENIMIENTO</h2>

<div>
    <h4>MANTENIMIENTO</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            ID MANTENIMIENTO:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.id_mante)
        </dd>

        <dt>
            COD PATRIMONIAL:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cod_patrimonial)
        </dd>

        <dt>
            COD TÉCNICO:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cod_tecnico)
        </dd>

        <dt>
            TIPO DE MANTENIMIENTO:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cod_tipoMante)
        </dd>

        <dt>
            OBSERVACIÓN DE MATENIMIENTO:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.obs_tipoMante)
        </dd>

        <dt>
            ESTADO DE EQUIPO:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.estado_equipo)
        </dd>

        <dt>
            FECHA DE MATENIMIENTO:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fecha_mante)
        </dd>

        <dt>
            ESTADO DE MANTENIMIENTO:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.estado_mante)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { /* id = Model.PrimaryKey */ }, new { @class = "btn btn-success" }) |
    @Html.ActionLink("Regresar", "listarMantenimientos", null, new { @class = "btn btn-primary" })
</p>
